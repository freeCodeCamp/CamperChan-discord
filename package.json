{
  "name": "discord-bot-test",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "npm run clean && prisma generate",
    "build": "tsc",
    "clean": "rimraf prod",
    "dev": "op run --env-file='./dev.env' -- tsx watch src/index.ts",
    "lint": "eslint src test --max-warnings 0 && prettier src test --check",
    "start": "op run --env-file='./prod.env' -- node prod/index.js",
    "test": "vitest run --coverage",
    "test:nocov": "MOCHA=true vitest run",
    "db:deploy": "prisma db push"
  },
  "engines": {
    "node": "20",
    "pnpm": "9"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "5.16.1",
    "@tryghost/admin-api": "1.13.12",
    "diff": "5.2.0",
    "discord.js": "14.15.3",
    "dotenv": "16.4.5",
    "highlight.js": "11.10.0",
    "mongodb": "6.8.0",
    "node-html-to-image": "4.0.0",
    "node-schedule": "2.1.1",
    "octokit": "4.0.2",
    "string-similarity": "4.0.4",
    "strip-ansi": "7.1.0",
    "winston": "3.13.1"
  },
  "devDependencies": {
    "@nhcarrigan/eslint-config": "3.3.3",
    "@nhcarrigan/prettier-config": "3.2.0",
    "@nhcarrigan/typescript-config": "4.0.0",
    "@types/diff": "5.2.1",
    "@types/node": "20.14.10",
    "@types/node-schedule": "2.1.7",
    "@types/prettier": "2.7.3",
    "@types/string-similarity": "4.0.2",
    "@vitest/coverage-istanbul": "1.6.0",
    "discordjs-testing": "0.2.0",
    "eslint": "8.57.0",
    "prettier": "3.3.2",
    "prisma": "5.16.1",
    "rimraf": "5.0.9",
    "tsx": "4.16.2",
    "typescript": "5.5.3",
    "vitest": "1.6.0"
  }
}
