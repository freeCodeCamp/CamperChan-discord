{
  "name": "discord-bot-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean && prisma generate",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint src test --max-warnings 0 && prettier src test --check",
    "start": "node -r dotenv/config dist/index.js",
    "pretest": "rimraf ./.nyc_output && ts-node test/validateCoverage.ts",
    "test": "nyc --check-coverage ts-mocha -u tdd  'test/**/*.spec.ts' --recursive --exit --timeout 10000",
    "test:nocov": "MOCHA=true ts-mocha -u tdd  'test/**/*.spec.ts' --recursive --exit --timeout 10000",
    "posttest": "ts-node test/validateCoverage.ts --post-coverage",
    "db:deploy": "prisma db push"
  },
  "engines": {
    "node": "20",
    "pnpm": "8"
  },
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "20.0.2",
    "@prisma/client": "5.11.0",
    "diff": "5.2.0",
    "discord.js": "14.14.1",
    "dotenv": "16.4.5",
    "highlight.js": "11.9.0",
    "mongodb": "6.5.0",
    "node-html-to-image": "4.0.0",
    "node-schedule": "2.1.1",
    "string-similarity": "4.0.4",
    "strip-ansi": "6.0.1",
    "winston": "3.12.0"
  },
  "devDependencies": {
    "@nhcarrigan/eslint-config": "3.2.0",
    "@nhcarrigan/prettier-config": "3.2.0",
    "@nhcarrigan/typescript-config": "3.0.0",
    "@types/chai": "4.3.14",
    "@types/diff": "5.0.9",
    "@types/mocha": "10.0.6",
    "@types/node": "20.11.30",
    "@types/node-schedule": "2.1.6",
    "@types/prettier": "2.7.3",
    "@types/string-similarity": "4.0.2",
    "chai": "4.4.1",
    "discordjs-testing": "0.2.0",
    "eslint": "8.57.0",
    "mocha": "10.3.0",
    "nyc": "15.1.0",
    "prettier": "3.2.5",
    "prisma": "5.11.0",
    "rimraf": "5.0.5",
    "ts-mocha": "9.0.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.3"
  }
}
