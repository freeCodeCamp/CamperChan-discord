{
  "name": "discord-bot-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint src test --max-warnings 0",
    "start": "node -r dotenv/config dist/index.js",
    "pretest": "rimraf ./.nyc_output && ts-node test/validateCoverage.ts",
    "test": "nyc --check-coverage ts-mocha -u tdd  'test/**/*.spec.ts' --recursive --exit --timeout 10000",
    "test:nocov": "MOCHA=true ts-mocha -u tdd  'test/**/*.spec.ts' --recursive --exit --timeout 10000",
    "posttest": "ts-node test/validateCoverage.ts --post-coverage",
    "db:deploy": "prisma db push"
  },
  "engines": {
    "node": "20",
    "pnpm": "8"
  },
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "20.0.2",
    "@prisma/client": "4.16.2",
    "diff": "5.1.0",
    "discord.js": "14.13.0",
    "dotenv": "16.3.1",
    "git-repo-info": "2.1.1",
    "highlight.js": "11.8.0",
    "luxon": "3.4.3",
    "string-similarity": "4.0.4",
    "strip-ansi": "6.0.1",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.6",
    "@types/common-tags": "1.8.2",
    "@types/diff": "5.0.5",
    "@types/luxon": "3.3.2",
    "@types/mocha": "10.0.2",
    "@types/prettier": "2.7.3",
    "@types/string-similarity": "4.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chai": "4.3.10",
    "discordjs-testing": "0.2.0",
    "editorconfig": "1.0.4",
    "eslint": "8.50.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "40.3.0",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "prettier": "2.8.8",
    "prisma": "4.16.2",
    "rimraf": "4.4.1",
    "ts-jest": "29.1.1",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
